{
    "name": "sunnysideup/templateoverview",
    "type": "silverstripe-vendormodule",
    "description": "adds development tools for building silverstripe websites, in particular around managing page types.",
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "templateoverview",
        "review",
        "page types",
        "template review"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4.0",
        "sunnysideup/prettyphoto": "*",
        "guzzlehttp/guzzle": "^6",
        "sebastian/diff": "*"
    },
    "suggest": {
        "sebastian/diff": "Show diff for html files"
    },
    "extra": {
        "expose": [
            "client/javascript",
            "client/css"
        ]
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\TemplateOverview\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
